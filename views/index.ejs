            <div class="text-center mb-5">
                <h2 class="display-4 fw-bold">Our Happy Customers</h2>
                <p class="lead">See how our customers enjoy Click Fit and share their amazing moments.</p>
            </div>

            <div class="text-center mb-4">
                <form id="uploadImageForm">
                    <label for="uploadImage" class="btn btn-primary">
                        <i class="fa-solid fa-upload"></i> Upload Your Image
                    </label>
                    <input type="file" id="uploadImage" accept="image/*" class="d-none">
                </form>
            </div>
    
            <div id="imageGallery" class="row g-4">
                <!-- Image 1 -->
                <div class="col-md-4">
                    <div class="media-card">
                        <a href="./images/person1.jpg" data-lightbox="imageGallery">
                            <img src="./images/person1.jpg" class="img-fluid rounded shadow" alt="Person 1">
                        </a>
                    </div>
                </div>
    
                <!-- Image 2 -->
                <div class="col-md-4">
                    <div class="media-card">
                        <a href="./images/person2.jpg" data-lightbox="imageGallery">
                            <img src="./images/person2.jpg" class="img-fluid rounded shadow" alt="Person 2">
                        </a>
                    </div>
                </div>
    
                <!-- Image 3 -->
                <div class="col-md-4">
                    <div class="media-card">
                        <a href="./images/person3.jpg" data-lightbox="imageGallery">
                            <img src="./images/person3.jpg" class="img-fluid rounded shadow" alt="Image 3">
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- Contact -->
    <section id="contact" class="py-5 bg-white">
        <div class="container">
            <div class="text-center mb-5">
                <h2 class="display-4 fw-bold">Contact Us</h2>
                <p class="lead">Have questions? Get in touch with us!</p>
            </div>
    
            <div class="row justify-content-center">
                <div class="col-md-8">
                    <form id="contactForm">
                        <div class="mb-3">
                            <label for="name" class="form-label">Your Name</label>
                            <input type="text" class="form-control" id="name" placeholder="Enter your name" required>
                        </div>
                        <div class="mb-3">
                            <label for="email" class="form-label">Your Email</label>
                            <input type="email" class="form-control" id="email" placeholder="Enter your email" required>
                        </div>
                        <div class="mb-3">
                            <label for="message" class="form-label">Your Message</label>
                            <textarea class="form-control" id="message" rows="4" placeholder="Type your message" required></textarea>
                        </div>
                        <div class="text-center">
                            <button type="submit" class="btn btn-success"><i class="fa-solid fa-paper-plane"></i> Send Message</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </section>
    
